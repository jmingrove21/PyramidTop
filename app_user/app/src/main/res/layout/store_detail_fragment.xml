<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_above="@+id/bottom_navigation"
    android:layout_marginTop="?attr/actionBarSize">

    <TextView
        android:id="@+id/store_rest"
        android:layout_width="62dp"
        android:layout_height="27dp"
        android:layout_alignStart="@+id/store_building_name"
        android:layout_alignLeft="@+id/store_building_name"
        android:layout_alignTop="@+id/store_building_name"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="338dp"
        android:layout_marginRight="338dp"
        android:text="휴무일: "
        android:textSize="20sp"></TextView>

    <TextView
        android:id="@+id/store_rest_day"
        android:layout_width="274dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/store_building_name"
        android:layout_alignLeft="@+id/store_building_name"
        android:layout_alignTop="@+id/store_building_name"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="87dp"
        android:layout_marginLeft="87dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="25dp"
        android:layout_marginRight="25dp"
        android:text="TextView"
        android:textSize="20sp"></TextView>

    <TextView
        android:id="@+id/store_branch_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/store_image"
        android:layout_alignBottom="@+id/store_image"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="9dp"
        android:layout_marginTop="-1dp"
        android:layout_marginEnd="328dp"
        android:layout_marginRight="328dp"
        android:layout_marginBottom="-27dp"
        android:fontFamily="sans-serif"
        android:text="TextView"
        android:textSize="18sp"></TextView>

    <TextView
        android:id="@+id/store_address"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:layout_alignStart="@+id/store_branch_name"
        android:layout_alignTop="@+id/store_branch_name"
        android:layout_alignBottom="@+id/store_branch_name"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="78dp"
        android:layout_marginEnd="-2dp"
        android:layout_marginRight="-2dp"
        android:layout_marginBottom="-78dp"
        android:fontFamily="sans-serif"
        android:text="TextView"
        android:textSize="18sp"></TextView>

    <TextView
        android:id="@+id/store_operation"
        android:layout_width="95dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/store_address"
        android:layout_alignTop="@+id/store_address"
        android:layout_alignBottom="@+id/store_address"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="307dp"
        android:layout_marginRight="307dp"
        android:layout_marginBottom="-42dp"
        android:fontFamily="sans-serif"
        android:text="운영시간:"
        android:textSize="18sp"></TextView>

    <TextView
        android:id="@+id/store_operation_start_time"
        android:layout_width="wrap_content"
        android:layout_height="28dp"
        android:layout_alignStart="@+id/store_operation"
        android:layout_alignTop="@+id/store_address"
        android:layout_alignBottom="@+id/store_address"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="102dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="189dp"
        android:layout_marginRight="189dp"
        android:layout_marginBottom="-49dp"
        android:fontFamily="sans-serif"
        android:text="TextView"
        android:textSize="18sp"></TextView>

    <TextView
        android:id="@+id/store_operation_wave"
        android:layout_width="22dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/store_operation_start_time"
        android:layout_alignTop="@+id/store_address"
        android:layout_alignBottom="@+id/store_address"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="123dp"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="152dp"
        android:layout_marginRight="152dp"
        android:layout_marginBottom="-41dp"
        android:fontFamily="sans-serif"
        android:text="~"
        android:textSize="18sp"></TextView>

    <TextView
        android:id="@+id/store_operation_end_time"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:layout_alignStart="@+id/store_address"
        android:layout_alignTop="@+id/store_address"
        android:layout_alignBottom="@+id/store_address"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="270dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="31dp"
        android:layout_marginRight="31dp"
        android:layout_marginBottom="-52dp"
        android:fontFamily="sans-serif"
        android:text="TextView"
        android:textSize="18sp"></TextView>

    <TextView
        android:id="@+id/store_notice"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/store_operation"
        android:layout_alignTop="@+id/store_operation"
        android:layout_alignBottom="@+id/store_operation"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="-79dp"
        android:fontFamily="sans-serif"
        android:text="TextView"
        android:textSize="18sp"></TextView>
</RelativeLayout>
