<?xml version="1.0" encoding="utf-8"?>

<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ScrollView1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_above="@+id/bottom_navigation">

<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <TextView
        android:id="@+id/store_branch_name"
        android:layout_width="401dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="9dp"
        android:layout_marginRight="328dp"
        android:fontFamily="sans-serif"
        android:text="TextView"
        android:textSize="18sp"></TextView>

    <TextView
        android:id="@+id/store_rest"
        android:layout_width="125dp"
        android:layout_height="42dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="286dp"
        android:layout_marginRight="338dp"
        android:text="휴무일: "
        android:textSize="20sp"></TextView>

    <TextView
        android:id="@+id/store_rest_day"
        android:layout_width="wrap_content"
        android:layout_height="39dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="134dp"
        android:layout_marginLeft="87dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="50dp"
        android:layout_marginRight="118dp"
        android:text="TextView"
        android:textSize="20sp"></TextView>

    <TextView
        android:id="@+id/store_address"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="87dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="-2dp"
        android:fontFamily="sans-serif"
        android:text="TextView"
        android:textSize="18sp"></TextView>

    <TextView
        android:id="@+id/store_operation"
        android:layout_width="125dp"
        android:layout_height="34dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="142dp"
        android:layout_marginEnd="285dp"
        android:layout_marginRight="307dp"
        android:fontFamily="sans-serif"
        android:text="운영시간:"
        android:textSize="18sp"></TextView>

    <TextView
        android:id="@+id/store_operation_start_time"
        android:layout_width="131dp"
        android:layout_height="37dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="57dp"
        android:layout_marginTop="190dp"
        android:layout_marginEnd="223dp"
        android:layout_marginRight="189dp"
        android:fontFamily="sans-serif"
        android:text="TextView"
        android:textSize="18sp"></TextView>

    <TextView
        android:id="@+id/store_operation_wave"
        android:layout_width="51dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="197dp"
        android:layout_marginTop="190dp"
        android:layout_marginEnd="192dp"
        android:layout_marginRight="152dp"
        android:fontFamily="sans-serif"
        android:text="~"
        android:textSize="18sp"></TextView>

    <TextView
        android:id="@+id/store_operation_end_time"
        android:layout_width="118dp"
        android:layout_height="38dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="274dp"
        android:layout_marginTop="190dp"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="31dp"
        android:fontFamily="sans-serif"
        android:text="TextView"
        android:textSize="18sp"></TextView>

    <TextView
        android:id="@+id/store_notice"
        android:layout_width="match_parent"
        android:layout_height="98dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="232dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:fontFamily="sans-serif"
        android:text="TextView"
        android:textSize="18sp"></TextView>
</RelativeLayout>
</ScrollView>
